fn generate_operator(operator String) -> (Int8, Int8) -> Int8:
    case operator:
        "+":
            fn (arg1 Int8, arg2 Int8) -> Int8: arg1 + arg2 end
        end

        "-":
            fn (arg1 Int8, arg2 Int8) -> Int8: arg1 - arg2 end
        end
        
        "*":
            fn (arg1 Int8, arg2 Int8) -> Int8: arg1 * arg2 end
        end
    end
end